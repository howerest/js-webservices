{
  "version": "2.2.2",
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./built",
    "sourceMap": true,
    "moduleResolution": "node"
  },
  "filesGlob": [
    "src/**/*.ts"
  ],
  "exclude": [
    "typings/",
    "node_modules/"
  ],
  "atom": {
    "rewriteTsconfig": false
  }
}
