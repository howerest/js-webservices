{
  "version": "1.5.0-alpha",
  "compilerOptions": {
    "target": "es3",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./built"
  },
  "filesGlob": [
    "./ts/**/*.ts",
    "!./node_modules/**/*.ts"
  ],
  "atom": {
    "rewriteTsconfig": false
  }
}
