{
  "version": "1.5.0-alpha",
  "compilerOptions": {
    "target": "es3",
    "module": "amd",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./js"
  },
  "filesGlob": [
    "./typings/**/*.ts",
    "./ts/**/*.ts",
    "./spec/**/*.ts",
    "!./node_modules/**/*.ts"
  ],
  "files": [
    "./typings/es6-promise/es6-promise.d.ts",
    "./typings/jasmine/jasmine.d.ts",
    "./typings/jasmine/legacy/jasmine-1.3.d.ts",
    "./typings/node/node.d.ts",
    "./typings/tsd.d.ts",
    "./ts/util.ts",
    "./ts/web_services.ts",
    "./ts/spec/web_services_spec.ts"
  ],
  "atom": {
    "rewriteTsconfig": false
  }
}
